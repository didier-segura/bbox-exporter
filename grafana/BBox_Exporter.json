{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "8.4.2"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": ""
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": {
          "limit": 100,
          "matchAny": false,
          "tags": [],
          "type": "dashboard"
        },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "Hardware",
      "id": 1,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "CPU Usage",
      "id": 2,
      "gridPos": { "h": 8, "w": 14, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "bb_device_cpu_user_percent", "legendFormat": "User", "refId": "A" },
        { "expr": "bb_device_cpu_system_percent", "legendFormat": "System", "refId": "B" },
        { "expr": "bb_device_cpu_idle_percent", "legendFormat": "Idle", "refId": "C", "hide": true },
        { "expr": "bb_device_cpu_usage_percent", "legendFormat": "User + System", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 35,
            "gradientMode": "opacity",
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "stat",
      "title": "CPU Temperature",
      "id": 3,
      "gridPos": { "h": 8, "w": 5, "x": 14, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "bb_device_cpu_temperature_main",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 60000 },
              { "color": "red", "value": 70000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["last"], "fields": "", "values": false },
        "orientation": "horizontal"
      }
    },
    {
      "type": "timeseries",
      "title": "Memory",
      "id": 4,
      "gridPos": { "h": 8, "w": 5, "x": 19, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "bb_device_mem_total", "legendFormat": "Total", "refId": "A" },
        { "expr": "bb_device_mem_free", "legendFormat": "Free", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 35,
            "gradientMode": "opacity",
            "lineInterpolation": "smooth",
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "row",
      "title": "WAN",
      "id": 5,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 }
    },
    {
      "type": "timeseries",
      "title": "WAN Throughput (Mbit/s)",
      "id": 6,
      "gridPos": { "h": 8, "w": 14, "x": 0, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "rate(bb_wan_ip_stats_rx_bytes[5m]) * 8", "legendFormat": "RX", "refId": "A" },
        { "expr": "rate(bb_wan_ip_stats_tx_bytes[5m]) * 8", "legendFormat": "TX", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 35,
            "gradientMode": "opacity",
            "lineInterpolation": "smooth",
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "stat",
      "title": "WAN Contractual Bandwidth (Mbit/s)",
      "id": 7,
      "gridPos": { "h": 4, "w": 5, "x": 14, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "bb_wan_ip_stats_rx_contractual_bandwidth", "legendFormat": "RX", "refId": "A" },
        { "expr": "bb_wan_ip_stats_tx_contractual_bandwidth", "legendFormat": "TX", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bps" },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["last"], "fields": "", "values": false } }
    },
    {
      "type": "stat",
      "title": "WAN State",
      "id": 8,
      "gridPos": { "h": 4, "w": 5, "x": 19, "y": 10 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "bb_wan_ip_state_up", "legendFormat": "IP Up", "refId": "A" },
        { "expr": "bb_wan_internet_state", "legendFormat": "Internet State", "refId": "B" },
        { "expr": "bb_wan_interface_state", "legendFormat": "Interface State", "refId": "C" },
        { "expr": "bb_wan_cgnat_enabled", "legendFormat": "CGNAT Enabled", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "Down" }, "1": { "text": "Up" } } }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["last"], "fields": "", "values": false } }
    },
    {
      "type": "table",
      "title": "WAN IP Info",
      "id": 9,
      "gridPos": { "h": 5, "w": 24, "x": 0, "y": 18 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "bb_wan_ip_info",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": []
      },
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true
            },
            "indexByName": {},
            "renameByName": {}
          }
        }
      ],
      "fieldConfig": {
        "defaults": { "custom": { "align": "auto" } },
        "overrides": []
      }
    },
    {
      "type": "row",
      "title": "LAN / Wi-Fi",
      "id": 10,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 23 }
    },
    {
      "type": "timeseries",
      "title": "LAN Throughput (Mbit/s)",
      "id": 11,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "rate(bb_lan_stats_rx_bytes[5m]) * 8", "legendFormat": "RX", "refId": "A" },
        { "expr": "rate(bb_lan_stats_tx_bytes[5m]) * 8", "legendFormat": "TX", "refId": "B" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 35,
            "gradientMode": "opacity",
            "lineInterpolation": "smooth",
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "timeseries",
      "title": "Wi-Fi Throughput (Mbit/s)",
      "id": 12,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        { "expr": "rate(bb_wireless_24_stats_rx_bytes[5m]) * 8", "legendFormat": "RX 2.4GHz", "refId": "A" },
        { "expr": "rate(bb_wireless_24_stats_tx_bytes[5m]) * 8", "legendFormat": "TX 2.4GHz", "refId": "B" },
        { "expr": "rate(bb_wireless_5_stats_rx_bytes[5m]) * 8", "legendFormat": "RX 5GHz", "refId": "C" },
        { "expr": "rate(bb_wireless_5_stats_tx_bytes[5m]) * 8", "legendFormat": "TX 5GHz", "refId": "D" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 35,
            "gradientMode": "opacity",
            "lineInterpolation": "smooth",
            "showPoints": "auto"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single", "sort": "none" }
      }
    },
    {
      "type": "nodeGraph",
      "title": "Network Topology (Mbit/s)",
      "id": 13,
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "label_replace(label_replace(bb_wan_ip_stats_rx_mbps, \"source\", \"Internet\", \"\", \"\"), \"target\", \"BBox\", \"\", \"\") or label_replace(label_replace(bb_wan_ip_stats_tx_mbps, \"source\", \"BBox\", \"\", \"\"), \"target\", \"Internet\", \"\", \"\")",
          "legendFormat": "",
          "refId": "A",
          "instant": true,
          "format": "table"
        },
        {
          "expr": "label_replace(label_replace(label_replace(vector(1), \"id\", \"BBox\", \"__name__\", \".*\"), \"title\", \"BBox\", \"__name__\", \".*\"), \"subTitle\", \"Gateway\", \"__name__\", \".*\") or label_replace(label_replace(label_replace(vector(1), \"id\", \"Internet\", \"__name__\", \".*\"), \"title\", \"Internet\", \"__name__\", \".*\"), \"subTitle\", \"Upstream\", \"__name__\", \".*\")",
          "legendFormat": "",
          "refId": "B",
          "instant": true,
          "format": "table"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Mbps",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "layout": "force",
        "edges": { "mainStatUnit": "Mbit/s" },
        "nodeSize": "lg",
        "showArrows": true,
        "showLabels": true,
        "showLegend": true
      },
      "transformations": [
        { "id": "labelsToFields", "options": {} },
        {
          "id": "organize",
          "options": {
            "excludeByName": { "Time": true, "__name__": true, "instance": true, "job": true },
            "includeByName": { "source": true, "target": true, "id": true, "title": true, "subTitle": true, "Value": true },
            "renameByName": { "Value": "mainStat" },
            "indexByName": { "source": 0, "target": 1, "id": 0, "Value": 2 }
          }
        }
      ]
    }
  ],
  "refresh": "1m",
  "schemaVersion": 35,
  "style": "dark",
  "tags": ["bbox", "prometheus"],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "BBox Exporter (Go)",
  "uid": "bbox-go",
  "version": 1,
  "weekStart": ""
}
